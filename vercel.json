{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.6.0"
    },
    {
      "src": "api/run_bot_task.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/run_bot_task",
      "dest": "/api/run_bot_task.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ]
}
