{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "api/run_bot_task.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/run_bot_task",
      "dest": "/api/run_bot_task.py"
    },
    {
      "src": "/test-gold/(.*)",
      "dest": "/api/$1.php"
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    }
  ]
}
